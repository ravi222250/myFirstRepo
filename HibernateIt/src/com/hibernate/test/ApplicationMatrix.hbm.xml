<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.hibernate.test.ApplicationMatrix" table="mytechub_matrix">
		<meta attribute="class-description"> This class contains details. </meta>
		
		<id name="serial_number" type="int" column="serial_number">
			<generator class="native" />
		</id>
		
		<property name="date_of_data" type="string" column="date_of_data" not-null="true" />
		<property name="time_of_data" type="string" column="time_of_data" not-null="true" />
		<property name="install_time" type="integer" column="install_time" not-null="true" />
		<property name="first_load_time" type="integer" column="first_load_time" not-null="true" />
		<property name="login_time" type="integer" column="login_time" not-null="true" />
		<property name="registration_time" type="integer" column="registration_time" not-null="true" />
		
	</class>
</hibernate-mapping>